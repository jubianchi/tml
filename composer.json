{
    "name": "jubianchi/compiler-sandbox",
    "require": {
        "php": "^7.0",
        "hoa/compiler": "*",
        "hoa/cli": "*",
        "hoa/console": "*"
    },
    "authors": [
        {
            "name": "jubianchi",
            "email": "contact@jubianchi.fr"
        }
    ],
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "jubianchi\\TML\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jubianhi\\TML\\Tests\\Units\\": "tests/units/"
        }
    },
    "require-dev": {
        "atoum/atoum": "*"
    }
}
